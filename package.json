{
  "name": "reseller-api",
  "version": "1.0.0",
  "description": "Boticario Reseller Api",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon src/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" src/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"src/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"src/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "migration": "ts-node ./node_modules/typeorm/cli.js migration:run"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "auth0": "^2.30.0",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-openapi-validator": "^4.5.0",
    "jwks-rsa": "^1.11.0",
    "pino": "^6.7.0",
    "sqlite3": "^5.0.0",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@types/auth0": "^2.20.9",
    "@types/express": "^4.17.8",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/pino": "^6.3.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.10",
    "nodemon": "^2.0.6",
    "pino-pretty": "^4.3.0",
    "prettier": "^2.1.2",
    "shelljs": "^0.8.4",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "author": "Igor Quirino <iquirino91@gmail.com> (https://github.com/iquirino)"
